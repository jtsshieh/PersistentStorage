{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ]
        }
      ],
      "dest": "../../docs/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ],
        "src": "../../docs"
      },
      {
        "files": [
          "articles/**.md",
          "articles/**.yml",
          "toc.yml",
          "**.md"
        ],
        "src": "../../docs"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "src": "../../docs"
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ],
        "src": "../../docs"
      }
    ],
    "dest": "../../docs/_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false
  }
}
